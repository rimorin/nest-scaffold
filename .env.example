# Application
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/nest_scaffold?schema=public"

# JWT Authentication
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=60m

# Cookie settings
COOKIE_SECRET=your_cookie_secret_key_here
COOKIE_DOMAIN=localhost

# CORS settings
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_USERNAME=

# BullMQ Queue settings
BULL_JOB_ATTEMPTS=3
BULL_REMOVE_ON_COMPLETE=100
BULL_REMOVE_ON_FAIL=200

# Cache settings
CACHE_TTL=5000
CACHE_MAX_ITEMS=100
